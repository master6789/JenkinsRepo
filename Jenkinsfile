pipeline {
  agent any
    stages {
      stage('Build'){
        steps{
          echo "Building ...."
        }
        
      }
      stage('Testing'){
        steps{
          echo "Testing ....."
        }
        
      
      }
      stage('Deploying'){
        steps{
          echo "Deploying.."
        }
        
      
      }
      echo "pipeline was successfull"
    }
}
